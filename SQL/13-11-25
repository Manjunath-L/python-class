SELECT SAL,dno
FROM emp
ORDER BY SAL DESC,dno ASC;

SELECT FNAME,LNAME,JOB
FROM emp
WHERE job in ('SALESMAN','MANAGER')
ORDER BY FNAME ASC;

SELECT *
FROM emp
LIMIT 3;


SELECT *
FROM emp
LIMIT 1;

SELECT *
FROM emp
LIMIT 2 OFFSET 2;


SELECT * 
FROM emp
ORDER BY SAL DESC
LIMIT 5;

SELECT DISTINCT SAL
FROM emp
ORDER BY SAL DESC
LIMIT 1 OFFSET 2;


SELECT DISTINCT SAL
FROM emp
ORDER BY SAL ASC
LIMIT 1 OFFSET 3;


SELECT LOWER('PENTGONE')
SELECT UPPER('PENTGONE')
SELECT LENGTH('PENTGONE SPANE')


SELECT CONCAT("MR/MISS ",FNAME,"YOUR SALARY IS ",SAL, " RS'") AS "EMP DITALS"
FROM emp;

SELECT CONCAT(FNAME," ",LNAME) AS "FULL NAME OF EMP"
FROM emp;

